{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "env-cmd": "^10.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^1.1.5",
    "socket.io": "^2.4.1",
    "socket.io-client": "^2.4.0"
  },
  "scripts": {
    "start-client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start": "cd ./server && npm run-script start --scripts-prepend-node-path",
    "build-prod": "env-cmd -f .env.production.local yarn run build && (cd server && yarn prod)",
    "build-dev": "env-cmd -f .env.development.local yarn run build && (cd server && yarn dev)",
    "portainer": "cd server && yarn prod"
  },
  "eslintConfig": {
    "extends": "react-app",
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": [
      "warn",
      {
        "aspects": [
          "invalidHref"
        ]
      }
    ]
  },
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
